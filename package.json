{
  "name": "STEMN",
  "productName": "STEMN",
  "version": "0.0.4",
  "description": "Painless Engineering Versioning",
  "repository" : { 
    "type" : "git",
    "url"  : "https://github.com/Stemn/Stemn-Desktop"
  },
  "main": "dist/main/index.js",
  "scripts": {
    "test"            : "cross-env NODE_ENV=test mocha --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch"      : "npm test -- --watch",
    "test-e2e"        : "cross-env NODE_ENV=test mocha --compilers js:babel-register --require ./test/setup.js --require co-mocha ./test/e2e.js",
    "lint"            : "eslint app test *.js",
    "build"           : "better-npm-run build:main && better-npm-run build:renderer",
    "build:main"      : "better-npm-run build:main",
    "build:renderer"  : "better-npm-run build:renderer",
    "app"             : "better-npm-run app",
    "app:test"        : "better-npm-run app:test",
    "server"          : "better-npm-run server",
    "server:test"     : "better-npm-run server:test",
    "compile"         : "node ./scripts/build.js"
  },
  "betterScripts": {
    "build:main": {
      "command": "node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js  --progress --profile --colors && cp app/package.json dist/",
      "env": {
        "NODE_ENV": "production",
        "API_SERVER": "54.70.226.125",
        "WEBSOCKET_SERVER": "54.70.226.125"
      }
    },    
    "build:renderer": {
      "command": "node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors && cp -R app/renderer/assets dist/renderer",
      "env": {
        "NODE_ENV": "production",
        "API_SERVER": "54.70.226.125",
        "WEBSOCKET_SERVER": "54.70.226.125"
      }
    },   
    "app": {
      "command": "electron -r babel-register -r babel-polyfill ./app/main/index",
      "env": {
        "HOT": "1",
        "NODE_ENV": "development",
        "API_SERVER": "localhost:3000",
        "WEBSOCKET_SERVER": "localhost"
      }
    },           
    "app:test": {
      "command": "electron -r babel-register -r babel-polyfill ./app/main/index",
      "env": {
        "HOT": "1",
        "NODE_ENV": "production",
        "API_SERVER": "54.70.226.125",
        "WEBSOCKET_SERVER": "54.70.226.125"
      }
    },  
    "server": {
      "command": "node --max-old-space-size=4096 -r babel-register server.js",
      "env": {
        "API_SERVER": "localhost:3000",
        "WEBSOCKET_SERVER": "localhost"
      }
    },
    "server:test": {
      "command": "node --max-old-space-size=4096 -r babel-register server.js",
      "env": {
        "API_SERVER": "54.70.226.125",
        "WEBSOCKET_SERVER": "54.70.226.125"
      }
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "author": {
    "name": "STEMN Pty Ltd",
    "email": "desktop@stemn.com"
  },
  "license": "MIT",
  "keywords": [
    "project management",
    "tracking",
    "versioning",
    "engineering",
    "science"
  ],
  "homepage": "https://stemn.com",
  "bugs": {
    "url": "https://stemn.com",
    "email": "bugs@stemn.com"
  },
  "dependencies": {
    "asar": "^0.11.0",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "better-npm-run": "0.0.13",
    "chai": "^3.5.0",
    "chromedriver": "^2.21.2",
    "co-mocha": "^1.1.2",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "devtron": "^1.2.1",
    "electron-rebuild": "^1.1.3",
    "es6-promise": "^3.3.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.6.0",
    "file-loader": "^0.9.0",
    "happypack": "^2.2.1",
    "jsdom": "^8.4.0",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "node-libs-browser": "^1.0.0",
    "node-monkey": "^1.0.0-beta.2",
    "react": "^15.2.1",
    "react-addons-test-utils": "^15.0.1",
    "selenium-webdriver": "^2.53.1",
    "sinon": "^1.17.3",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webfont-dl": "^0.1.2",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.10.0"
  },
  "devEngines": {
    "node": "4.x || 5.x || 6.x",
    "npm": "2.x || 3.x"
  },
  "contributors": [
    "David Revay <david@stemn.com>",
    "Jackson Delahunt <jackson@stemn.com>"
  ],
  "build": {
    "appId": "stemn.stemn",
    "app-bundle-id": "stemn.stemn",
    "app-category-type": "public.app-category.productivity",
    "productName": "STEMN",
    "compression": "normal",
    "asar": true,
    "win": {
      "iconUrl": "https://stemn.com/uploads/85c7df7e-312b-4ed9-b12f-11d6d001a220.png"
    },
    "osx": {
      "contents": [
        {
          "x": 448,
          "y": 344,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 192,
          "y": 344,
          "type": "file",
          "path": "Stemn.app"
        }
      ]
    }
  },
  "directories": {
    "app": "./dist/",
    "output": "./release/"
  },
  "devDependencies": {
    "cross-env": "^1.0.8",
    "electron": "^1.4.4",
    "electron-builder": "^7.3.0"
  }
}
